    }
   },
   "outputs": [],
   "source": [
    "rice1 = ARP_clean['ricetotal_q'].quantile(0.25)\n",
    "rice2 = ARP_clean['ricetotal_q'].quantile(0.75)\n",
    "iqr_rice = rice2-rice1\n",
    "up_limit = rice2 + 1.5*iqr_rice\n",
    "low_limit = rice1 - 1.5*iqr_rice"
   ]
  },
